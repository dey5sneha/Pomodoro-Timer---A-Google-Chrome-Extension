{
    "manifest_version": 3,
    "name": "Pomodoro Timer",
    "description": "Save your time and get things done faster",
    "version": "1.0.0",
    "icons": {
        "16": "icons/alarm-clock-16.png",
        "32": "icons/alarm-clock-32.png",
        "48": "icons/alarm-clock-48.png",
        "128": "icons/alarm-clock-128.png"
    },
    "action": {
        "default_icon":{
        "16": "icons/alarm-clock-16.png",
        "32": "icons/alarm-clock-32.png",
        "48": "icons/alarm-clock-48.png",
        "128": "icons/alarm-clock-128.png"
        }
    },
    "permissions": ["contextMenus", "alarms", "notifications"],
    "background": {
        "service_worker": "service_worker.js"
    }
}